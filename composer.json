{
  "name": "cortex/universities",
  "description": "Cortex Universities is a Laravel package that integrates the rinvex/universities dataset into a relational database.",
  "type": "cortex-module",
  "keywords": [
    "rinvex",
    "cortex",
    "flexible",
    "framework",
    "laravel",
    "themes",
    "solution",
    "extension",
    "enterprise",
    "application",
    "multi-lingual",
    "multi-tenant",
    "bootstrap",
    "module",
    "Universities",
    "education",
    "dataset"
  ],
  "license": "MIT",
  "homepage": "https://github.com/karimsamir/cortex-universities",
  "support": {
    "issues": "https://github.com/karimsamir/cortex-universities/issues",
    "source": "https://github.com/karimsamir/cortex-universities"
  },
  "authors": [
    {
      "name": "Karim Samir",
      "email": "karim@example.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/database": "^10.0.0 || ^11.0.0 || ^12.0.0",
    "illuminate/support": "^10.0.0 || ^11.0.0 || ^12.0.0",
    "illuminate/console": "^10.0.0 || ^11.0.0 || ^12.0.0",
    "rinvex/universities": "^6.0.0",
    "cortex/foundation": "^8.0.0",
    "league/fractal": "^0.20.0",
    "rinvex/laravel-composer": "7.5.*"
  },
  "version": "1.0.0",
  "require-dev": {
    "phpunit/phpunit": "^10.1.0"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Cortex\\Universities\\": "src/",
      "Cortex\\Universities\\Database\\Factories\\": "database/factories/",
      "Cortex\\Universities\\Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cortex\\Universities\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "post-update-cmd": [
      "php artisan migrate",
      "php artisan db:seed --class='Cortex\\Universities\\Database\\Seeders\\UniversitiesSeeder'",
      "chmod -R 777 storage"
    ]
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Cortex\\Universities\\Providers\\UniversitiesServiceProvider"
      ]
    },
    "installer-paths": {
      "custom/path/to/package/": [
        "app/modules/cortex"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
